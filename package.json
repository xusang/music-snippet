{
	"name": "music-snippet",
	"displayName": "music-snippet",
	"description": "code-snippet",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"icon": "icon.png",
	"publisher": "xusang",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:music-snippet.musicSnippets"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "music-snippet.musicSnippets",
				"title": "music snippets"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets.json"
			},
			{
				"language": "typescript",
				"path": "./snippets.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets.json"
			},
		],
		"configuration": {
			"properties": {
				"music-snippet.token": {
					"type": "string",
					"default": "9Rxk8YycErTSyqnsPkX8",
					"description": "Personal Access Tokens，gitlab中你的Access token"
				},
				"music-snippet.url": {
					"type": "string",
					"default": "https://g.hz.netease.com",
					"description": "gitlab的Url"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"axios": "^0.24.0"
	}
}
